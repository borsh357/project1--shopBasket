{
  "name": "webpack-dev-env",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "validate-css": "src/app.css",
  "jshint": "src/app.js",
  "scripts": {
    "server": "webpack-dev-server",
    "validate-html": "html-validate src/index.html",
    "validate-css": "css-validator src/app.css",
    "jshint": "jshint src/js/*.js",
    "watch": "webpack watch",
    "dev-mode": "npm-run-all server watch",
    "postinstall": "npm i -g live-server css-validator html-validate jshint",
    "build": "webpack -p",
    "valid-build": "npm-run-all validate-html validate-css jshint test build",
    "dpl": "gh-pages -d build",
    "deploy": "npm-run-all valid-build dpl",
    "test": "mocha --require @babel/register src/js/test.js"
  },
